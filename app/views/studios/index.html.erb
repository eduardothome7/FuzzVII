<p id="notice"><%= notice %></p>

<h1>Studios</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Social name</th>
      <th>Email</th>
      <th>Description</th>
      <th>User</th>
      <th>Cep</th>
      <th>Address</th>
      <th>City</th>
      <th>Uf</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @studios.each do |studio| %>
      <tr>
        <td><%= studio.name %></td>
        <td><%= studio.social_name %></td>
        <td><%= studio.email %></td>
        <td><%= studio.description %></td>
        <td><%= studio.user_id %></td>
        <td><%= studio.cep %></td>
        <td><%= studio.address %></td>
        <td><%= studio.city %></td>
        <td><%= studio.uf %></td>
        <td><%= studio.picture %></td>
        <td><%= link_to 'Show', studio %></td>
        <td><%= link_to 'Edit', edit_studio_path(studio) %></td>
        <td><%= link_to 'Destroy', studio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Studio', new_studio_path %>
